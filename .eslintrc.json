{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": 0,
        "no-console": 0,
        "react/jsx-indent": 0,
        "react/jsx-filename-extension": 0,
        "react/no-unused-state": 1,
        "react/no-unused-prop-types": 1,
        "linebreak-style": 0
    }
}
